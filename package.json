{
  "name": "webfolio",
  "description": "A portfolio landing page showcasing modern and creative web design projects.",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node scripts/reactor.mjs --dev && next dev --turbopack",
    "build": "node scripts/reactor.mjs --build && next build",
    "start": "node scripts/reactor.mjs --start && next start",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,css}\"",
    "types": "tsc --noEmit",
    "typecheck": "tsc --noEmit --incremental false",
    "clean": "trash node_modules && rm -rf pnpm-lock.yaml",
    "clean:next": "trash .next && trash .next/cache",
    "clean:build": "trash .next",
    "clean:cache": "trash .next/cache",
    "reboot": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "@eslint/js": "^9.26.0",
    "@hookform/resolvers": "^5.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.10.5",
    "geist": "^1.4.2",
    "lucide-react": "^0.509.0",
    "next": "^15.3.2",
    "next-intl": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.176.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.3.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "trash": "^9.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0"
  },
  "pnpm": {
    "overrides": {
      "next@>=15.0.0 <15.2.3": ">=15.2.3"
    }
  }
}
