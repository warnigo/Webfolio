{
  "name": "webfolio",
  "description": "A portfolio landing page showcasing modern and creative web design projects.",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": {
    "name": "Abubakir Shavkatov",
    "email": "me@warnigo.uz",
    "url": "https://warnigo.uz"
  },
  "scripts": {
    "dev": "node scripts/reactor.mjs --dev && next dev --turbopack",
    "build": "node scripts/reactor.mjs --build && next build",
    "start": "node scripts/reactor.mjs --start && next start",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,css}\"",
    "types": "tsc --noEmit",
    "typecheck": "tsc --noEmit --incremental false",
    "clean": "trash .next && trash .next/cache && trash node_modules && rm -rf pnpm-lock.yaml",
    "clean:build": "trash .next",
    "clean:cache": "trash .next/cache",
    "reboot": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "@hookform/resolvers": "^3.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.15",
    "framer-motion": "^11.18.2",
    "geist": "^1.4.2",
    "lucide-react": "^0.471.2",
    "next": "^15.5.2",
    "next-intl": "^3.26.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.5.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "trash": "^9.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=21.0.0",
    "npm": ">=7.0.0"
  }
}
